{
  "name": "molecular",
  "version": "0.0.1",
  "icon": "./docs/molecular.png",
  "description": "Small framework to easily bind angular and electron toghether.",
  "main": "build/molecular.js",
  "scripts": {
    "build": "webpack",
    "build-prod": "webpack -p",
    "deploy": "npm run build && sh deploy.sh ",
    "watch": "webpack --progress --colors --watch",
    "lint": "tslint 'src/**/*.ts' 'demo/src/**/*.ts'",
    "test": "npm run lint",
    "pass": "echo 'it passes'",
    "doc": "typedoc --exclude '**/build/**' --out docs/jsdoc/ ./ && cp molecular-128.png docs/jsdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rylphs/molecular.git"
  },
  "keywords": [
    "angular",
    "electron"
  ],
  "author": "Raphael Couto Ferreira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rylphs/molecular/issues"
  },
  "homepage": "https://github.com/rylphs/molecular#readme",
  "types": "build/molecular.d.ts",
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.10",
    "awesome-typescript-loader": "^3.2.1",
    "electron": "^1.6.11",
    "electron-packager": "^8.7.2",
    "electron-rebuild": "^1.5.11",
    "glob": "^7.1.1",
    "jasmine": "^2.6.0",
    "pre-commit": "^1.2.2",
    "tslint": "^5.0.0",
    "tslint-config-standard": "^6.0.1",
    "typedoc": "^0.7.1",
    "typescript": "^2.1.4",
    "vanilla-typescript": "^0.3.2",
    "webpack": "^3.3.0",
    "@angular/core": "4.1.3",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.1",
    "zone.js": "0.8.4"
  },
  "peerDependencies": {
    "@angular/core": ">=4.1.3",
    "reflect-metadata": ">=0.1.8",
    "rxjs": ">=5.0.1",
    "zone.js": ">=0.8.4"
  }
}
